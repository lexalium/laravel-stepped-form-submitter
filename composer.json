{
    "name": "lexal/laravel-stepped-form-submitter",
    "description": "Stepped Form submitter for Laravel & Lumen.",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Lex",
            "email": "allapkov@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "lumen",
        "form submitter",
        "stepped form submitter",
        "laravel stepped form submitter",
        "laravel multi form submitter"
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/contracts": "^9.0 || ^10.0",
        "illuminate/support": "^9.0 || ^10.0",
        "lexal/form-submitter": "^2.0",
        "lexal/stepped-form": "^2.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "roave/security-advisories": "dev-latest",
        "webimpress/coding-standard": "^1.3"
    },
    "suggest": {
        "illuminate/config": "Required to use Laravel config (^9.0 || ^10.0).",
        "illuminate/events": "Required to listen Stepped Form event (^9.0 || ^10.0)."
    },
    "autoload": {
        "psr-4": {
            "Lexal\\LaravelSteppedFormSubmitter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lexal\\LaravelSteppedFormSubmitter\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lexal\\LaravelSteppedFormSubmitter\\ServiceProvider\\ServiceProvider"
            ]
        }
    }
}
